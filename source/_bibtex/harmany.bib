	Journal = {{IEEE} Transactions on Image Processing},
	Keywords = {algorithm;sparse, approximation;sparse, approximation;spatially, approximations;quadratic, constraint;optimization, {data;Poisson}, distributed, estimation, formulation;partition-based, function;nonnegativity, function;penalized, function;quadratic, Gaussian, intensity, least, log-likelihood, method;penalized, methods;least, {model;Poisson}, {model;SPIRAL-TAP;coefficient}, multiscale, negative, noise, noise;approximation, objective, phenomenon;temporally, phenomenon;total, Poisson, problem;iteration, processes;, programming;stochastic, reconstruction, reconstruction;iterative, {seminorm;Gaussian}, square, squares, theory;image, variation, vector;inverse},
	Month = mar,
