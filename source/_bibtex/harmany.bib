	Bdsk-Url-1 = {http://dx.doi.org/10.1109/TIP.2011.2168410}}

@inproceedings{hernandez2011bounded-gradient-projection-methods-for-sparse,
	Author = {Hernandez, James and Harmany, Zachary T. and Thompson, Daniel O. and Marcia, Roummel F.},
	Booktitle = {2011 {IEEE} International Conference on Acoustics, Speech and Signal Processing {(ICASSP)}},
	Date-Added = {2012-01-10 17:33:57 +0000},
