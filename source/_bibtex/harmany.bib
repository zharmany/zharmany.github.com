@inproceedings{harmany2009sparse-poisson-intensity-reconstruction-algorithms,
	Address = {Cardiff, United Kingdom},
	Author = {Harmany, Zachary T. and Marcia, Roummel F. and Willett, Rebecca M.},
	Booktitle = {2009 {IEEE/SP} 15th Workshop on Statistical Signal Processing, {SSP} '09},
	Date-Added = {2010-05-05 04:18:44 -0700},
